<%= form_for @listing do |f| %>
	<p id="new_listing_name">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>

	<p id="new_listing_description">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</p>

	<p id="new_listing_address">
		<%= f.label :address %>
		<%= f.text_field :address %>
	</p>

	<p id="new_listing_country">
		<%= f.label :country %>
		<%= f.text_field :country %>
	</p>

	<p id="new_listing_city">
		<%= f.label :city %>
		<%= f.text_field :city %>
	</p>

	<p id="new_listing_property_type">
		<%= f.label :property_type %>
		<%= f.select :property_type, [['Apartment','Apartment'],['House','House'],['Condominium','Condominium']] %>
	</p>

	<p id="new_listing_accomodates">
		<%= f.label :accomodates %>
		<%= f.number_field :accomodates %>
	</p>

	<p id="new_listing_bedrooms">
		<%= f.label :bedrooms %>
		<%= f.number_field :bedrooms %>
	</p>

		<p id="new_listing_bathrooms">
		<%= f.label :bathrooms %>
		<%= f.number_field :bathrooms %>
	</p>

	<p id="new_listing_price">
		<%= f.label :price %>
		<%= f.number_field :price %>
	</p>

	<p><%= f.collection_check_boxes(:tag_ids, Tag.all, :id, :name) %></p>

	<%= f.file_field :avatars, multiple: true %>


	<p id="new_listing_submit_button">
		<%= f.submit %>
	</p>



<%end%> 