

<%= form_for @listing do |f| %>
	<p id="new_listing_">
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>

	<p id="new_listing_">
		<%= f.label :description %>
		<%= f.text_field :description %>
	</p>

	<p id="new_listing_">
		<%= f.label :address %>
		<%= f.text_field :address %>
	</p>

	<p id="new_listing_">
		<%= f.label :country %>
		<%= f.text_field :country %>
	</p>

	<p id="new_listing_">
		<%= f.label :city %>
		<%= f.text_field :city %>
	</p>

	<p id="new_listing_">
		<%= f.label :price %>
		<%= f.number_field :price %>
	</p>
	<p id="new_listing_">
		<%= f.submit %>
	</p>

<%end%> 